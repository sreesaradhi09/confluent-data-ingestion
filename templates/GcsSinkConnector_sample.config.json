{
  "name": "GcsSinkConnector_sample",
  "cloud.environment": "prod",
  "kafka.endpoint": "SASL_SSL://abc:9092",
  "kafka.region": "us-east4",
  "kafka.service.account.id": "sa-xxxxx",
  "topics.dir": "sink",
  "tasks.max": "1",
  "topics": "t1,t2",
  "gcs.credentials.config": "secret://gcs/sink/creds",
  "gcs.bucket.name": "bkt-d-use4-gcs-landing",
  "input.data.format": "JSON",
  "output.data.format": "JSON",
  "time.interval": "HOURLY",
  "timezone": "US/Eastern",
  "transforms": "MaskField",
  "transforms.MaskField.fields": "SSN",
  "transforms.MaskField.type": "org.apache.kafka.connect.transforms.MaskField$Value",
  "flush.size": "100000",
  "rotate.interval.ms": "900000",
  "time.partition.duration.ms": "3600000",
  "time.path.format": "'year'=YYYY/'month'=MM/'day'=dd/'hour'=HH",
  "filename.template": "${topic}-${timestamp}.json",
  "compression": "gz",
  "errors.tolerance": "all",
  "errors.log.enable": "true",
  "errors.deadletterqueue.context.headers.enable": "true",
  "cloud.provider": "gcp",
  "kafka.auth.mode": "SERVICE_ACCOUNT",
  "connector.class": "GcsSink"
}
